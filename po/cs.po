msgid "Download rate:"
msgstr "Rychlost stahování:"

#: src/salix-codecs-installer-gtk:269
msgid "Reading package lists..."
msgstr "Načítání seznamu balíčku..."

#: src/salix-codecs-installer-gtk:299
msgid "Retrieving package data..."
msgstr "Získávání dat o balíčku..."

#: src/salix-codecs-installer-gtk:318
msgid "Retrieving patch list..."
msgstr "Získávání seznamu oprav..."

#: src/salix-codecs-installer-gtk:336
msgid "Retrieving checksum list..."
msgstr "Získávání seznamu kontrolních součtů..."

#: src/salix-codecs-installer-gtk:354
msgid "Retrieving checksum signature..."
msgstr "Získávání podpisu kontrolních součtů..."

#: src/salix-codecs-installer-gtk:364
msgid "Verifying checksum signature..."
msgstr "Ověřování kontrolního součtu podpisu..."

#: src/salix-codecs-installer-gtk:375
msgid "Retrieving ChangeLog.txt..."
msgstr "Získávání souboru změn - ChangeLog.txt..."

#: src/salix-codecs-installer-gtk:501
msgid "Updating local package cache"
msgstr "Aktualizace lokální balíčkovací paměti"

#: src/salix-codecs-installer-gtk:566
msgid "Downloading packages"
msgstr "Stahování balíčků"

#: src/salix-codecs-installer-gtk:617
msgid "Installing packages"
msgstr "Instalování balíčků"

#: src/salix-codecs-installer-gtk:753
msgid "Name"
msgstr "Název"

#: src/salix-codecs-installer-gtk:756
msgid "Version"
msgstr "Verze"

#: src/salix-codecs-installer-gtk:759
msgid "Description"
msgstr "Popis"

